{
    "presets": [
        "@babel/typescript",
        "@babel/preset-env",
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      ["@babel/plugin-proposal-decorators", {"decoratorsBeforeExport": true}],
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ],
    "only": [
        "./src",
    ],
    "sourceMaps": true,
	"retainLines": true
}
